{
  "name": "lovecrypto",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i",
    "android:build": "cd android && ./gradlew assembleRelease && open './app/build/outputs/apk/'",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "git-hook": "npm run lint -s && npm run test -s",
    "fixcode": "eslint --fix ./app"
  },
  "dependencies": {
    "@eva-design/eva": "2.0.0",
    "@invertase/react-native-apple-authentication": "^2.1.0",
    "@miblanchard/react-native-slider": "^2.0.2",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/google-signin": "^5.0.0",
    "@react-native-firebase/analytics": "^10.0.0",
    "@react-native-firebase/app": "^10.0.0",
    "@react-native-firebase/auth": "^10.0.0",
    "@react-native-firebase/messaging": "^10.0.0",
    "@react-native-firebase/storage": "^10.0.0",
    "@react-native-masked-view/masked-view": "^0.2.6",
    "@react-navigation/bottom-tabs": "^5.8.0",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/stack": "^6.0.11",
    "@ui-kitten/components": "5.0.0",
    "@ui-kitten/eva-icons": "5.0.0",
    "@ui-kitten/moment": "^5.0.0",
    "async": "^3.2.2",
    "axios": "^0.19.2",
    "formik": "^2.1.5",
    "lottie-ios": "3.1.8",
    "lottie-react-native": "^4.0.2",
    "moment": "^2.27.0",
    "react": "16.13.1",
    "react-native": "0.63.3",
    "react-native-animatable": "^1.3.3",
    "react-native-animated-pull-to-refresh": "^1.0.3",
    "react-native-animated-splash-screen": "^2.0.5",
    "react-native-app-intro-slider": "^4.0.4",
    "react-native-chart-kit": "^6.11.0",
    "react-native-currency-input": "^1.0.1",
    "react-native-fbsdk": "^3.0.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-image-picker": "^3.0.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-localization": "^2.1.6",
    "react-native-localize": "^1.4.3",
    "react-native-masked-text": "^1.13.0",
    "react-native-paper": "^4.8.1",
    "react-native-parallax-header": "^1.1.4",
    "react-native-progress": "^5.0.0",
    "react-native-safe-area-context": "^3.1.6",
    "react-native-safe-area-view": "^1.1.1",
    "react-native-screens": "^2.10.1",
    "react-native-selection-group": "^2.0.0",
    "react-native-shimmer-placeholder": "^1.0.36",
    "react-native-svg": "^12.1.1",
    "react-native-tiny-toast": "^1.0.7",
    "react-native-unimodules": "^0.12.0",
    "react-native-vector-icons": "^7.1.0",
    "react-native-video": "^5.1.0-alpha8",
    "react-native-video-player": "^0.10.1",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "use-count-up": "^2.3.1",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^1.1.0",
    "@ui-kitten/metro-config": "5.0.0",
    "eslint": "^7.7.0",
    "jest": "^25.1.0",
    "prettier": "^2.0.5",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": []
  }
}
